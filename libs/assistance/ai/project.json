{
  "name": "assistance-ai",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/assistance/ai/tools/src",
  "projectType": "library",
  "tags": ["feature"],
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/assistance/ai/tools",
        "tsConfig": "libs/assistance/ai/tools/tsconfig.lib.json",
        "project": "libs/assistance/ai/tools/package.json",
        "entryFile": "libs/assistance/ai/tools/src/index.ts",
        "external": ["react", "react-dom", "react/jsx-runtime"],
        "rollupConfig": "@nx/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "libs/assistance/ai/tools/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    }
  }
}
