                    "description": "Type of the module"
                },
                "description": {
                    "type": "string",
                    "description": "Description of the module"
                },
                "tags": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "description": "Tags associated with the module"
                },
                "children": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "description": "Children of the module"
                },
                "readme": {
                    "type": "object",
                    "additionalProperties": {
                        "type": "string"
                    },
